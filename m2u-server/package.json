{
  "name": "m2u-server",
  "version": "1.0.1",
  "description": "m2u-server",
  "keywords": ["fastify", "mobilab2u"],
  "license": "MIT",
  "author": "Santheepkumar",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsup",
    "build:ts": "tsup --env.NODE_ENV production",
    "check": "biome check --write .",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "copy:assets": "cpy 'src/generated/*.json' dist/generated && cpy 'src/templates/*.hbs' dist/templates",
    "dev": "pnpm compile-schemas && pnpm dev:watch",
    "dev:watch": "tsx watch src/server.ts",
    "seed": "tsup && node ./dist/seeder/index.js",
    "seed:fake": "node ./dist/seeder/fake/index.js",
    "format": "biome format . --write",
    "lint": "biome lint .",
    "migrate:eslint": "biome migrate eslint --write",
    "migrate:prettier": "biome migrate prettier --write",
    "start": "tsx src/server.ts",
    "start:prod": "node dist/server.js",
    "local:prod": "tsup && node dist/server.js",
    "test": "cross-env TS_NODE_FILES=true tap test/**/*.test.ts",
    "typecheck": "tsc -noEmit",
    "prepare": "cd .. && husky m2u-server/.husky",
    "migrateDb": "tsup && node ./dist/migrateDbForUserManagement.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.609.0",
    "@casl/ability": "^6.7.1",
    "@casl/mongoose": "^8.0.2",
    "@faker-js/faker": "^8.4.1",
    "@fastify/autoload": "5.10.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "9.0.1",
    "@fastify/env": "4.4.0",
    "@fastify/etag": "5.2.0",
    "@fastify/helmet": "11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "8.3.0",
    "@fastify/rate-limit": "9.1.0",
    "@fastify/sensible": "5.6.0",
    "@fastify/swagger": "8.14.0",
    "@fastify/swagger-ui": "4.0.0",
    "@hyperdx/node-opentelemetry": "0.8.0",
    "@immobiliarelabs/fastify-sentry": "^8.0.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.47.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.52.1",
    "@opentelemetry/exporter-trace-otlp-proto": "0.52.1",
    "@opentelemetry/sdk-metrics": "1.25.1",
    "@opentelemetry/sdk-node": "0.52.1",
    "@sinclair/typebox": "0.32.34",
    "@types/nodemailer": "^6.4.15",
    "ajv": "6",
    "altair-fastify-plugin": "7.2.1",
    "axios": "^1.7.9",
    "base-64": "^1.0.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "close-with-grace": "1.3.0",
    "dotenv": "16.4.5",
    "fastify": "4.28.1",
    "fastify-cli": "6.2.1",
    "fastify-plugin": "4.5.1",
    "fastify-raw-body": "^4.3.0",
    "fastify-tsconfig": "2.0.0",
    "graphql": "16.9.0",
    "handlebars": "^4.7.8",
    "joi": "^17.13.3",
    "mercurius": "14.1.0",
    "mercurius-codegen": "5.0.5",
    "mercurius-integration-testing": "8.2.0",
    "mongo-aggregation-utils": "^1.2.8",
    "mongoose": "^8.4.4",
    "nexus": "1.3.0",
    "node-cron": "^4.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "otp-generator": "^4.0.1",
    "puppeteer": "^22.12.1",
    "stripe": "^16.5.0",
    "swagger-jsdoc": "6.2.8",
    "twilio": "^5.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@tapjs/test": "3.0.2",
    "@types/base-64": "^1.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/busboy": "1.5.4",
    "@types/mongoose-sequence": "^3.0.11",
    "@types/node": "20.14.9",
    "@types/otp-generator": "^4.0.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/tap": "15.0.11",
    "cpy-cli": "5.0.0",
    "cross-env": "7.0.3",
    "fast-json-stringify": "6.0.0",
    "husky": "^9.1.4",
    "json-schema-to-typescript": "14.1.0",
    "lint-staged": "^15.2.7",
    "sort-package-json": "2.10.0",
    "tap": "20.0.2",
    "ts-node": "10.9.2",
    "tsup": "8.1.0",
    "tsx": "4.16.0",
    "typescript": "5.5.2"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=18.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,html}": ["pnpm format"]
  },
  "tsup": {
    "entry": ["src/**/*.ts"],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": ["cjs", "esm"],
    "legacyOutput": true,
    "target": "es2018",
    "onSuccess": "npm run copy:assets"
  }
}
